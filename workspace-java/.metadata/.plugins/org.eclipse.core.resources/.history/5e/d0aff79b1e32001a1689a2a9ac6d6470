public class AppQueTestaTV {
	public static void main(String args[]) {
		Televisao t1;          // declarei que t1 vai "manipular" uma Tv
		t1 = new Televisao();  // reservo um espaço na memória para isso
		
		t1.marca = "Sony";
		t1.voltagem = 110;
		t1.tamanho = 32;
		t1.ligada = false;
		t1.canal = 5;
		t1.volume = 2;
		
		t1.switchPower();
		t1.switchPower();
		t1.switchPower();
		
	}

}
