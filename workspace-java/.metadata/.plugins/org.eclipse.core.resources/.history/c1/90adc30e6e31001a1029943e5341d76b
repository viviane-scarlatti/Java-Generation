import java.util.Scanner;
public class Uri1060 {
	public static void main(String args[]) {
		Scanner teclado;
		teclado = new Scanner(System.in);
		
		float x=0;
		int cont=0;
		
		for (cont=1; cont <=6; cont++) {
			x=teclado.nextFloat();
		}
			if (x>0) {
				cont++;
				System.out.println(cont+" valores positivos");
			}
		teclado.close();
	}
}
