import java.util.Scanner;
public class Uri1060 {
	public static void main(String args[]) {
		Scanner teclado;
		teclado = new Scanner(System.in);
		
		float i;
		int cont=0;
		
		//for (i=1; i <=6; i++) {
		while (i<6) {
			i=teclado.nextFloat();
		}
			if (i>0) {
				cont++;
				System.out.println(cont+" valores positivos");
			}
		
		teclado.close();
	}
}
