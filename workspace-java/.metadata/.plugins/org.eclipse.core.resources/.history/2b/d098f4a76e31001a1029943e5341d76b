import java.util.Scanner;
public class Uri1060 {
	public static void main(String args[]) {
		Scanner teclado;
		teclado = new Scanner(System.in);
		
		float x=0;
		int i=0, cont=0;
		
		for (i=1; i <=6; i++) {
			x=teclado.nextFloat();
		}
			if (x>0f) {
				cont++;
				System.out.println(cont+" valores positivos");
			}
		teclado.close();
	}
}
